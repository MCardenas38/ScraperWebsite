<h1 style="text-decoration: underline;">Scraped Headlines</h1>
<br><br>
{{#each result}}
<div class="row boxes">
    <div class="row">
        <div class="col-sm-9">
            <h3 class="title">{{title}}</h3>
        </div>
        <div class="col-sm-3">
            <h6 class="time">{{time}}</h6>
            <button id="archive" class="btn btn-warning">Archive</button>
        </div>
    </div>
    <div class="row">
        <p class="link"><a href={{link}}>{{link}}</a></p>
    </div>
</div>
{{/each}}